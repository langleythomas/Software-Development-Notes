  "editor.bracketPairColorization.enabled": true,
  "editor.cursorBlinking": "phase",
  "editor.cursorStyle": "line",
  "editor.fontSize": 14,
  "editor.guides.indentation": true,
  "editor.indentSize": "tabSize",
  "editor.insertSpaces": true,
  "editor.minimap.showSlider": "always",
  "editor.showFoldingControls": "always",
  "editor.stickyScroll.enabled": true,
  "editor.tabSize": 2,

  "extensions.experimental.affinity": {
    "yzhang.markdown-all-in-one": 1
  },

  "explorer.compactFolders": false,

  // "files.dialog.defaultPath": "/home/thomas/Repos", // Linux
  // "files.dialog.defaultPath": "C:\\Repos", // Windows
  "files.insertFinalNewline": true,

  "markdown-pdf.headerTemplate": "<div style=\"font-size: 9px; margin-left: 1cm;\"></div>",

  "markdown.extension.toc.updateOnSave": true,

  "markdownlint.config": {
    "MD013": {
      "code_blocks": false,
      "headings": false,
      "line_length": 120,
      "strict": true,
      "tables": false
    },
    "MD025": false,
    "MD033": false,
    "MD041": false
  },

  "telemetry.telemetryLevel": "off",

  "terminal.integrated.allowChords": false,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorStyleInactive": "line",
  "terminal.integrated.defaultProfile.linux": "bash",
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.middleClickBehavior": "paste",
  "terminal.integrated.stickyScroll.enabled": true,

  // "vscode-neovim.neovimExecutablePaths.linux": "nvim",

  "workbench.activityBar.location": "top",
  "workbench.colorTheme": "Dracula Pro Edition",
  "workbench.iconTheme": "city-lights-icons-vsc-light",
  "workbench.preferredDarkColorTheme": "Dracula Pro Edition",
  "workbench.sideBar.location": "right",
  "workbench.tree.enableStickyScroll": true,
  "workbench.tree.renderIndentGuides": "always"
}
